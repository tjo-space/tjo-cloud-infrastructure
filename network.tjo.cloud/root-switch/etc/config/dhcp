config dnsmasq
  # Disable DNS
  option port '0'

config dhcp 'local'
	option interface 'local'
  option dhcpv6 relay
  option ra relay
  option ndp relay

config dhcp 'internal'
  option interface 'internal'
  option dhcpv6 relay
  option ra relay
  option ndp relay
  option master 1

config odhcpd 'odhcpd'
	option maindhcp '0'
	option loglevel '4'
	option piofolder '/tmp/odhcpd-piofolder'
